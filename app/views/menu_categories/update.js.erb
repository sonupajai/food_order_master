$("#ul_errors").html("")
<% if @menu_category.errors.any? %>
  	<% @menu_category.errors.full_messages.each do |message| %>
    	$("#ul_errors").append($("<li />").html("<%= message.html_safe %>"))
	<% end %>
<% else %>
	$("#menu_view").html("<%= escape_javascript(render 'index') %>")
	$("#menu_category_name").val("");
<% end %>
