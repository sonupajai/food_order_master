$("#city_name").html("")
<% if @city.errors.any? %>
  	<% @city.errors.full_messages.each do |message| %>
    	$("#ul_errors").append($("<li />").html("<%= message.html_safe %>"))
	<% end %>
<% else %>

	$(".new-city").html("<%= escape_javascript(render 'form') %>")
	$("#city_name").val("<%=  @city.name %>")
<% end %>